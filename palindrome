class Solution {
    int isPlaindrome(String S) 
    {
        int n=S.length();
        int left=0;
        int right=n-1;
        int p=0;
        String s[]=new String[n];
        for(int i=0;i<n;i++)
        {
            s[i]=S[i];
        }
        while(right>left)
        {
            
           if(s[left++]==s[right--]);
            {
            p=1;
            }
        }
        return p;
        
        
      
    }
};
